<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="gc_ip_timestamp_view_tree" model="ir.ui.view">
        <field name="name">gc.ip.timestamp.tree</field>
        <field name="model">gc.ip.timestamp</field>
        <field name="arch" type="xml">
            <tree>
                <field name="user_id" />
                <field name="ip_id" />
                <field name="create_date" />
            </tree>
        </field>
    </record>

    <record id="gc_ip_timestamp_view_form" model="ir.ui.view">
        <field name="name">gc.ip.timestamp.form</field>
        <field name="model">gc.ip.timestamp</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="user_id" />
                        <field name="ip_id" />
                        <field name="create_date" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>
