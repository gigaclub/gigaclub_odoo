<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>

        <record model="ir.ui.view" id="gc_user_form_view_inherit_builder_system">
            <field name="name">gc.user.from.view.inherit</field>
            <field name="model">gc.user</field>
            <field
        name="inherit_id"
        ref="gigaclub_team.gc_user_form_view_inherit_team"
      />
            <field name="arch" type="xml">
                <field name="state" position="after">
                    <field name="world_ids" />
                    <field name="world_manager_ids" />
                </field>
            </field>
        </record>

        <record model="ir.ui.view" id="gc_user_tree_view_inherit_builder_system">
            <field name="name">gc.user.tree.view.inherit</field>
            <field name="model">gc.user</field>
            <field
        name="inherit_id"
        ref="gigaclub_team.gc_user_tree_view_inherit_team"
      />
            <field name="arch" type="xml">
                <field name="state" position="after">
                    <field name="world_ids" />
                    <field name="world_manager_ids" />
                </field>
            </field>
        </record>

    </data>
</odoo>
