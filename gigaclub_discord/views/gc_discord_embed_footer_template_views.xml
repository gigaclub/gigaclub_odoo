<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="gc_discord_embed_footer_template_view_form" model="ir.ui.view">
        <field name="name">gc.discord.embed.footer.template.form</field>
        <field name="model">gc.discord.embed.footer.template</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="text" />
                        <field name="icon_url" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="gc_discord_embed_footer_template_view_tree" model="ir.ui.view">
        <field name="name">gc.discord.embed.footer.template.tree</field>
        <field name="model">gc.discord.embed.footer.template</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="text" />
                <field name="icon_url" />
            </tree>
        </field>
    </record>

</odoo>
