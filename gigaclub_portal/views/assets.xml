<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>

        <template id="assets_frontend_inherit_gigaclub_portal" inherit_id="web.assets_frontend" priority="20">
            <xpath expr="//link[1]" position="before">
                <link
              rel="stylesheet"
              type="text/css"
              href="/gigaclub_portal/static/libs/css/bootstrap-select.css"
            />
            </xpath>
        </template>

        <template
      id="_assets_frontend_minimal_js_inherit_gigaclub_portal"
      inherit_id="web._assets_frontend_minimal_js"
      priority="20"
    >
            <xpath expr="//script[1]" position="before">
                <script
          type="text/javascript"
          src="/gigaclub_portal/static/libs/js/bootstrap-select.js"
        ></script>
            </xpath>
        </template>

    </data>
</odoo>
